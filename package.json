{
  "name": "@solvei/borsh",
  "version": "0.0.18",
  "readme": "README.md",
  "homepage": "https://github.com/the-solvei/borsh-ts#README",
  "description": "Binary Object Representation Serializer for Hashing",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "yarn build:dev",
    "build:dev": "tsc -p ./tsconfig.json",
    "build:prod": "rimraf dist && tsc -p ./tsconfig.prod.json && cp package.json dist/ && cp README.md dist/ && cp LICENSE-APACHE dist/ && cp LICENSE-MIT.txt dist/ ",
    "test": "jest test --runInBand",
    "fuzz": "jsfuzz src/test/fuzz/borsh-roundtrip.js src/test/fuzz/corpus/",
    "pretest": "yarn build",
    "lint": "eslint src/**/*.ts",
    "pretty": "prettier --write src/**/*.ts package.json",
    "pretty:check": "yarn prettier --loglevel error --check  src/**/*.ts package.json",
    "fix": "eslint src/**/*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-solvei/borsh-ts.git"
  },
  "keywords": [
    "serializer",
    "binary",
    "serializer",
    "deserializer",
    "consistency",
    "deterministic"
  ],
  "author": "The Solvei",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/the-solvei/borsh-ts/issues"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.2",
    "@types/babel__template": "^7.0.2",
    "@types/bn.js": "^5.1.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.7.3",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "bs58": "^4.0.0",
    "eslint": "^6.5.1",
    "jest": "^27.0.0",
    "js-sha256": "^0.9.0",
    "jsfuzz": "^1.0.14",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "bn.js": "^5.2.0",
    "bs58": "^4.0.0",
    "text-encoding-utf-8": "^1.0.2"
  }
}
